{
  "name": "real-estate-crawler",
  "version": "1.0.0",
  "description": "Playwright-based crawler for real estate listings",
  "main": "index.js",
  "scripts": {
    "test-debug": "playwright test --ui --project chromium",
    "test": "playwright test",
    "crawl": "ts-node -e \"(async()=>{const {runCrawler}=await import('./src/crawler');const criteria=require('./search.json');await runCrawler(criteria,{headless:false});})();\"",
    "server": "ts-node src/server.ts",
    "prepare": "npx playwright install"
  },
  "dependencies": {
    "@playwright/test": "^1.54.2",
    "@types/cors": "^2.8.19",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "moment": "^2.29.4",
    "playwright": "^1.40.0",
    "ts-node": "^10.9.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^24.2.0",
    "typescript": "^5.9.2"
  }
}
